<Window x:Class="CompressedFileViewer.Windows.DecompressDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CompressedFileViewer.Windows"
        mc:Ignorable="d"
        Title="DecompressDialog" Height="453" Width="800" WindowStyle="ToolWindow">
    <Grid>
        <ListView Margin="10,10,10,40" Name="lstAlg"/>
        <Button Margin="10,0,0,10" Height="22" VerticalAlignment="Bottom" Content="Decompress" HorizontalAlignment="Left" Width="240" Click="Compress" />
        <Button Margin="550,0,0,10" Height="22" VerticalAlignment="Bottom" Content="Cancel" HorizontalAlignment="Left" Width="240" Click="Cancel"/>
    </Grid>
</Window>
